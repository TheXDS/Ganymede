<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="TheXDS.MCART.Security" Version="0.17.1-alpha.4" />
    <PackageReference Include="TheXDS.MCART.Security.Argon2" Version="0.17.1-alpha.4" />
    <PackageReference Include="TheXDS.ServicePool" Version="1.0.5" />
    <PackageReference Include="TheXDS.Triton.Diagnostics" Version="1.6.1-alpha3" />
    <PackageReference Include="TheXDS.Triton.Faker" Version="1.6.1-alpha3" />
    <PackageReference Include="TheXDS.Triton.InMemory" Version="1.6.1-alpha3" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\src\Bundles\Ganymede.Proteus\Ganymede.Proteus.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Resources\Strings\Models\User.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>User.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\Strings\Views\DialogDemoView.Designer.cs">
      <DependentUpon>DialogDemoView.resx</DependentUpon>
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
    </Compile>
    <Compile Update="Resources\Strings\Views\WelcomeView.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>WelcomeView.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Resources\Strings\Models\User.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>User.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\Strings\Views\DialogDemoView.resx">
      <LastGenOutput>DialogDemoView.Designer.cs</LastGenOutput>
      <Generator>PublicResXFileCodeGenerator</Generator>
      <CustomToolNamespace>TheXDS.Ganymede.Resources.Strings.Views</CustomToolNamespace>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\Strings\Views\WelcomeView.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>WelcomeView.Designer.cs</LastGenOutput>
      <CustomToolNamespace>TheXDS.Ganymede.Resources.Strings.Views</CustomToolNamespace>
    </EmbeddedResource>
  </ItemGroup>

</Project>
